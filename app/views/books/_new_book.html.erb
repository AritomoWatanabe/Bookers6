<h2>New book</h2>

<%= form_for(book, url: '/books') do |f| %>



<div class="field">
  <p class= "new-title">Titel</p>
  <%= f.text_field :title %>
</div>

<div class="field">
  <p class= "new-body">Body</p>
  <%= f.text_area :body %>
</div>

  <%= f.submit('Create Book',class: "btn btn-primary form-control") %>

<% end %>
